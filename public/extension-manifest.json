{
  "manifest_version": 2,
  "name": "Arancina is the only way",
  "version": "1.0.0",
  "description": "An auto dom fixer for specific arancina words",
  "author": "Daniele Cin√†",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"],
      "css": ["main.css"]
    }
  ],
  "web_accessible_resources": ["static/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
