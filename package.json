  "scripts": {
    "start": "react-app-rewired start",
    "build": "INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "test": "react-app-rewired test",
    "coverage": "yarn test --coverage --watchAll=false --coverageReporters=text-summary --coverageReporters=lcov",
    "lint": "eslint --ext=.js --ext=.ts --ext=.tsx src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/src/reportWebVitals",
      "/src/setupTests.js",
      "/src/testUtilies",
      "/src/index.ts",
      "/src/pages/background/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
